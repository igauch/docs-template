    "vue",
    "markdown",
    "md"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/igauch/docs-template/issues"
  },
  "homepage": "https://github.com/igauch/docs-template#readme",
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
